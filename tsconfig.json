{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"resolveJsonModule": true,
		"outDir": "./dist",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true
	}
}
