{
	"name": "node-wialon",
	"version": "0.14.0",
	"description": "A NodeJS wrapper implementation for Wialon Remote API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"typings": "dist/index.d.ts",
	"scripts": {
		"test:run": "jest --config jest.config.json",
		"test:watch": "jest --watch --config jest.config.json",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "tslint -p tsconfig.json",
		"build": "tsc",
		"prepare": "npm run build",
		"prepublishOnly": "npm run test:run && npm run lint",
		"preversion": "npm run lint",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ramilamparo/node-wialon.git"
	},
	"keywords": [
		"wialon"
	],
	"author": "Ramil Amparo",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ramilamparo/node-wialon/issues"
	},
	"homepage": "https://github.com/ramilamparo/node-wialon#readme",
	"devDependencies": {
		"@types/jest": "^24.0.18",
		"@types/nock": "^11.1.0",
		"jest": "^24.9.0",
		"nock": "^11.4.0",
		"prettier": "^1.18.2",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.4.1",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.6.4"
	},
	"dependencies": {
		"@types/node": "^12.7.12",
		"axios": "^0.19.0",
		"axios-mock-adapter": "^1.17.0",
		"form-data": "^2.5.1"
	},
	"files": [
		"dist/**/*"
	]
}
